{
  "manifest_version": 3,
  "name": "Toxic-Comment-Classification",
  "description": "An extension to give toxicity reports",
  "version": "2.0.1",
  "minimum_chrome_version": "49",
  "author": "SETeam10",
  "action": {
    "default_popup": "index.html",
    "default_title": "Toxic-Comment-Classification"
  },
  "web_accessible_resources": [
    {
      "resources": ["index.html"],
      "matches": ["https://*/*"]
    }
  ],
  "permissions": ["tabs", "storage", "webRequest", "webNavigation"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["style.css"],
      "js": ["js/jquery-3.3.1.min.js", "app.js", "js/chart_imported.js"]
    }
  ]
}
